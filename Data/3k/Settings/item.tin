#VAR {_item[^A burning funeral pyre$]}
{
    @fFormatItemData{
    {_area} {global}
	{desc}
	{
		{1} {A burning funeral pyre}
		{2} {You see a blazing bonfire burning in effigy of a fallen soul.  The}
		{3} {flames glow with a unique color, as though azure and white energies}
		{4} {were dancing in unison.  Standing in the light and warmth of the}
		{5} {comforting flames, you feel of revitalized and at peace, as if the}
		{6} {corpse's energy is somehow being transferred to you through death.}
	}
	{id} {funeral pyre}
	{nomap} {1}
	{name} {A burning funeral pyre}
	{pickup} {-1}
    }
};
#VAR {_item[^Ventra's secret chest \x28open\x29$]}
{
    @fFormatItemData{
    {_area} {global}
	{desc}
	{
		{1} {Ventra's secret chest (open)}
		{2} {You see a large chest made of lacquered oak wood with gold accents.}
		{3} {The item looks very large and can hold many things.}
		{4} {This chest belongs to Blutash.}
		{5} {The chest is open.}
	}
	{id} {chest}
	{name} {Ventra's secret chest (open)}
	{nomap} {1}
	{pickup} {-1}
    }
};
