#VAR {_item[containing \d+ messages]}
{
    @fFormatItemData{
    {_area} {global}
	{desc}
	{
		{1} {Type 'help board' for a list of commands.}
	}
	{id} {board}
	{name} {board}
	{pickup} {-1}
    }
};
#VAR {_item[^A single gold coin$]}
{
    @fFormatItemData{
    {_area} {global}
	{nomap} {1}
	{pickup} {-1}
    }
};
#VAR {_item[^A set of glowing red shackles, bound to]}
{
    @fFormatItemData{
    {_area} {global}
	{desc}
	{
		{1} {A set of glowing red shackles, bound to A Security Guard}
		{2} {A ghostly set of magical shackles.}
	}
	{id} {shackles}
	{nomap} {1}
	{pickup} {-1}    
    }
};
#VAR {_item[^A Trashcan$]}
{
    @fFormatItemData{
    {_area} {global}
	{desc}
	{
		{1} {A Trashcan}
		{2} {Use this trashcan to dispose of any garbage. By doing your part to}
		{3} {help make 3k a cleaner, thus better place, brings rewards to all}
		{4} {loyal citizens. To do this type: dispose <item> for single items}
		{5} {and dispose <all> for all unkept items.}
		{6} {Note: Typing dispose all will dispose all unkept items so make sure}
		{7} {you type keep <item> for the things you want to keep. Also}
		{8} {typing dispose all will not get rid of guild objects or other}
		{9} {items that cannot be dropped. Any problems mail Rastafan.}
	}
	{id} {trashcan}
	{name} {A Trashcan}
    }
};
